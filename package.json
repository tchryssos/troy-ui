    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^6.5.16",
    "ts-jest": "^29.0.3",
    "typescript": "^5.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
